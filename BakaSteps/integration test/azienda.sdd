azienda -> (nome) dirigenza, sede;
dirigenza -> cda, ceo, cto;
ceo -> (nome, cognome) retribuzione;
cto -> (nome, cognome) retribuzione;
cda -> componente_cda;
componente_cda -> (nome, cognome) retribuzione;
sede -> (citta) reparto;
reparto -> (nome) dipendente;
dipendente -> (grado, nome, cognome) retribuzione;
retribuzione -> benefit, stipendio;
stipendio -> (mensilita, importo_mensile);
benefit -> #PCDATA;
